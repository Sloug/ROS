<?xml version="1.0"?>

<launch>
	<include file="$(find husky_gazebo)/launch/husky_empty_world.launch"> 
	<arg name="world_name" value="worlds/robocup14_spl_field.world"/>
	<arg name="laser_enabled" value="true"/>
	</include>
	<node name="teleop" pkg="teleop_twist_keyboard" type="teleop_twist_keyboard.py" output="screen"/>
	<node name="rviz" pkg="rviz" type="rviz" output="screen" args="-d $(find husky_highlevel_controller)/config/aufgabe2.rviz"/>
	<node name="husky_highlevel_controller_node" pkg="husky_highlevel_controller" type="husky_highlevel_controller_node" output="screen">
	<rosparam command="load" file="$(find husky_highlevel_controller)/config/config.yaml" />
	</node>
</launch>
